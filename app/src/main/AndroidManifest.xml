<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.topQuizz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/quizz_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.topQuizz.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.topQuizz.QuestionsActivity">
            <intent-filter>
                <action android:name="com.topQuizz.QuestionsActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.topQuizz.DeveloperActivity">
            <intent-filter>
                <action android:name="com.topQuizz.DeveloperActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.topQuizz.ResultActivity"></activity>
    </application>

</manifest>